<h1>All Questions <%= link_to "Create New Question", new_question_path, class: 'btn btn-success' %></h1>

<% @questions.each do |question| %>
  <div class = "media">
    <div class = "media-body">
      <h4 class = "media-heading">
        <p><%= link_to "#{question.title}, Resolved: #{question.resolved}", question %></p>
      </h4>
    </div>
  </div>
<% end %>
